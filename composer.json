    "type": "composer-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {"DemosEurope\\DemosplanAddon\\": "src/"}
    },
    "extra": {
        "class": "DemosEurope\\DemosplanAddon\\AddonInstallerPlugin"
    },
    "require": {
